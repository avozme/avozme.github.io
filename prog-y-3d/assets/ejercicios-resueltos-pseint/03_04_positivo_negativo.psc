Proceso positivo_negativo
	Definir n como entero
	
	Escribir "Dime un número"
	Leer n
	
	si n > 0 Entonces
		escribir "El número es POSITIVO"
	FinSi
	
	si n < 0 Entonces
		escribir "El número es NEGATIVO"
	FinSi
	
	si n = 0 Entonces
		escribir "El número no es ni positivo ni negativo"
	FinSi
FinProceso
